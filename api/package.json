  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=dev node --import=tsx --watch --env-file=.env src/index.ts",
    "start": "node --require dotenv/config dist/src/index.js",
    "build": "tsc && node scripts/fix-imports.js",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed:update": "tsx src/db/updatePrefrence.ts",
    "db:reset": "tsx scripts/reset-db.ts && npm run db:push",
    "db:reset-simple": "npm run db:drop && npm run db:push",
    "db:drop": "drizzle-kit drop"
  },
